{
    "tools": [  
        {
            "title": "nmap TCP",
            "object": {   
                "tags": "enum port network tcp",
                "description": "All TCP port scan with NSE scripts",
                "command": "sudo nmap -sS -sV -sC -T4 -p- $ip",
                "args": [
                    "ip"
                ]
            }
        },
        {
            "title": "ping",
            "object": {   
                "tags": "network icmp",
                "description": "Send ICMP request to IP",
                "command": "ping $ip",
                "args": [
                    "ip"
                ]
            }
        },
        {
            "title": "netcat",
            "object": {   
                "tags": "banner grab port network tcp",
                "description": "TCP/UDP client to connect to any server",
                "command": "nc -v $ip $port",
                "args": [
                    "ip",
                    "port"
                ]
            }
        },
        {
            "title": "gobuster",
            "object": {   
                "tags": "enum http https directory web",
                "description": "Gobuster directory enumeration",
                "command": "gobuster dir -k -w $wordlist -u $url",
                "args": [
                    "wordlist",
                    "url"
                ]
            }
        }
    ]
}